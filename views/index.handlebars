<div>
  <div class="container">
    <div class="jumbotron text-center">
      <h1>Twitch.tv Web Scraper!</h1>
      <h3>Periodically updates page for most watched games on Twitch.tv</h3>
      <button class="btn btn-primary btn-lg" id="update">Update!</button>
      <p>Last updated:<span id="update_time"></span></p>
    </div>
    <div class="row">
      {{#each data}}
      <div class="col-md-4">
        <img src={{this.imageSrc}}>
        <h3>{{this.game}}</h1>
          <p>
            <span>Viewers:</span> {{this.viewers}}</p>
      </div>
      {{/each}}
    </div>
  </div>
</div>